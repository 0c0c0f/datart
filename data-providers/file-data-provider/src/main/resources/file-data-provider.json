{
  "type": "FILE",
  "name": "file-data-provider",
  "syntax": {
  },
  "attributes": [
    {
      "name": "cacheEnable",
      "type": "bool",
      "required": true,
      "defaultValue": true,
      "description": "开启后会将查询的数据保存到内部数据库"
    },
    {
      "name": "cacheTimeout",
      "type": "string",
      "required": true,
      "defaultValue": "30",
      "description": "缓存的过期时间（分钟）。设置为 -1 表示不过期，当修改数据源后，仍然会清空缓存。"
    },
    {
      "name": "schemas",
      "type": "files",
      "key": "tableName",
      "children": [
        {
          "name": "tableName",
          "required": true,
          "defaultValue": "",
          "type": "string"
        },
        {
          "name": "format",
          "required": true,
          "defaultValue": "",
          "type": "string",
          "description": "file format , csv or excel",
          "options": [
            "XLSX",
            "CSV"
          ]
        },
        {
          "name": "path",
          "required": true,
          "defaultValue": "",
          "type": "string",
          "description": "file path"
        },
        {
          "name": "columns",
          "defaultValue": "",
          "type": "schema"
        }
      ]
    }
  ]
}