{
  "type": "HTTP",
  "name": "http-data-provider",
  "syntax": {
  },
  "attributes": [
    {
      "name": "cacheEnable",
      "type": "bool",
      "required": true,
      "defaultValue": true,
      "description": "开启后会将查询的数据保存到内部数据库"
    },
    {
      "name": "cacheTimeout",
      "type": "string",
      "required": true,
      "defaultValue": "30",
      "description": "缓存的过期时间（分钟）"
    },
    {
      "name": "schemas",
      "type": "array",
      "key": "tableName",
      "children": [
        {
          "name": "tableName",
          "required": true,
          "type": "string"
        },
        {
          "name": "url",
          "required": true,
          "defaultValue": "http://",
          "type": "string"
        },
        {
          "name": "method",
          "required": true,
          "defaultValue": "GET",
          "type": "string"
        },
        {
          "name": "property",
          "defaultValue": "",
          "description": "The property name of the list data in JSON",
          "type": "string"
        },
        {
          "name": "columns",
          "defaultValue": "",
          "type": "schema"
        },
        {
          "name": "username",
          "defaultValue": "",
          "type": "string"
        },
        {
          "name": "password",
          "defaultValue": "",
          "type": "password"
        },
        {
          "name": "timeout",
          "defaultValue": 0,
          "description": "request timeout",
          "type": "number"
        },
        {
          "name": "responseParser",
          "defaultValue": "",
          "description": "Http Response Parser",
          "type": "string"
        },
        {
          "name": "headers",
          "type": "object"
        },
        {
          "name": "queryParam",
          "type": "object"
        },
        {
          "name": "body",
          "type": "string"
        },
        {
          "name": "contentType",
          "type": "string",
          "defaultValue": "application/json"
        }
      ]
    }
  ]
}